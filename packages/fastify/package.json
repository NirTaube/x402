{
  "name": "@x402/fastify",
  "version": "0.1.0",
  "type": "module",
  "description": "Fastify adapter for x402 middleware",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@x402/core": "workspace:*",
    "@x402/proxy": "workspace:*",
    "fastify-plugin": "^5.0.1"
  },
  "peerDependencies": {
    "fastify": ">=4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "fastify": "^5.2.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
